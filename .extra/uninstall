#!/bin/bash
set -e

if [[ -f "$installabs"/lib64/libcppextensions.a ]] || [[ -f "$installabs"/lib64/libcppextensions.so ]]; then
	rm -f -v "$installabs"/lib64/libcppextensions.a "$installabs"/lib64/libcppextensions.so
	echo "removing test objects, as they might be linked with -lcppextensions" >&2
	rm -f -v *.o
fi

cd libs && make uninstall
